-- NEW MAGIC COPY SECTION
uiSecs.MagicCopy = uiPages.Exploits:addSection("Magic Copy")

local SourceMagicName = ""
local TargetMagicName = ""

uiSecs.MagicCopy:addTextbox("Source Magic (copy FROM)", "Fire Magic", function(value)
    SourceMagicName = value
end)

uiSecs.MagicCopy:addTextbox("Target Magic (copy TO)", "Cataclysm Magic", function(value)
    TargetMagicName = value
end)

uiSecs.MagicCopy:addButton("ðŸ”„ Copy Magic!", function()
    local MagicModule = require(game.ReplicatedStorage.RS.Modules.Magic)
    
    if not MagicModule["Types"][SourceMagicName] then
        Veynx:Notify("Error!", "Source magic '" .. SourceMagicName .. "' not found!")
        return
    end
    
    if not MagicModule["Types"][TargetMagicName] then
        Veynx:Notify("Error!", "Target magic '" .. TargetMagicName .. "' not found!")
        return
    end
    
    -- Copy the entire magic type
    MagicModule["Types"][TargetMagicName] = MagicModule["Types"][SourceMagicName]
    
    -- Also copy variation 1 specifically (like your original code)
    if MagicModule["Types"][SourceMagicName]["Variations"] and 
       MagicModule["Types"][SourceMagicName]["Variations"]["1"] and
       MagicModule["Types"][TargetMagicName]["Variations"] then
        MagicModule["Types"][TargetMagicName]["Variations"]["1"] = MagicModule["Types"][SourceMagicName]["Variations"]["1"]
    end
    
    Veynx:Notify("Success!", "Copied " .. SourceMagicName .. " â†’ " .. TargetMagicName)
end)

uiSecs.MagicCopy:addButton("ðŸ“‹ List All Magics", function()
    local MagicModule = require(game.ReplicatedStorage.RS.Modules.Magic)
    local magicNames = {}
    
    for name, _ in pairs(MagicModule["Types"]) do
        table.insert(magicNames, name)
    end
    
    table.sort(magicNames)
    local magicList = table.concat(magicNames, ", ")
    Veynx:Notify("Magics Available:", magicList)
end)

